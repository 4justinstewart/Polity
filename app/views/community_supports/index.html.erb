<h1>Listing community_supports</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Legislation</th>
      <th>Support</th>
      <th>Text</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @community_supports.each do |community_support| %>
      <tr>
        <td><%= community_support.user %></td>
        <td><%= community_support.legislation %></td>
        <td><%= community_support.support %></td>
        <td><%= community_support.text %></td>
        <td><%= link_to 'Show', community_support %></td>
        <td><%= link_to 'Edit', edit_community_support_path(community_support) %></td>
        <td><%= link_to 'Destroy', community_support, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Community support', new_community_support_path %>
